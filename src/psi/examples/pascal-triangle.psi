#!/usr/bin/psyche

/n arguments 0 get tointeger def
/a [ 1 ] def

//n
{
	0 1 a length 1 sub
	{
		dup
		a exch get tostring print
		a length 1 sub ne '\t' '\n' ifelse print
	}
	for
	a 0 0 insert
	0 1 a length 2 sub
	{
		dup
		a exch 1 add get
		a 2 index get add
		a 3 1 roll
		put
	}
	for
}
bind repeat

stdout flush
